  "scripts": {
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.prod.js",
    "build:open": "yarn run build && http-server ./dist -p 4000 -o",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "flow": "flow check",
    "flow:watch": "flow",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "yarn run lint --watch",
    "prebuild": "yarn run lint && yarn run test && yarn run clean-dist",
    "serve": "webpack-serve --port 3000 --log-level error --config ./webpack.config.dev.js",
    "start": "concurrently -k -r -s first \"yarn run test:watch\" \"yarn run serve\" \"yarn run lint:watch\"",
    "test": "jest",
    "test:cover": "yarn run test --coverage",
    "test:watch": "jest --watch"
  },
