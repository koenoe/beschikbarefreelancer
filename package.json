{
  "name": "beschikbarefreelancer",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run serve\" \"npm run lint:watch\"",
    "serve": "webpack-serve --open --port 3000 --log-level error --config ./webpack.config.dev.js",
    "lint": "esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm run lint && npm run test && npm run clean-dist",
    "build": "NODE_ENV=production webpack -p --config ./webpack.config.prod.js",
    "build:open": "npm run build && http-server ./dist -p 4000 -o",
    "test": "jest",
    "test:cover": "npm run test -- --coverage ",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koenoe/beschikbarefreelancer.git"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "8.0.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.1",
    "babel-loader": "7.1.3",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.24.1",
    "concurrently": "3.5.1",
    "connect-history-api-fallback": "1.5.0",
    "cors": "2.8.4",
    "coveralls": "3.0.0",
    "css-loader": "0.28.10",
    "dotenv": "5.0.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "eslint-watch": "3.1.3",
    "express": "4.16.2",
    "express-http-proxy": "1.1.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.9",
    "gh-pages": "1.1.0",
    "history": "4.7.2",
    "html-webpack-plugin": "3.0.6",
    "http-server": "0.11.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.2",
    "jest-cli": "22.4.2",
    "jest-transform-stub": "1.0.0",
    "json-loader": "0.5.7",
    "mockdate": "2.0.2",
    "node-sass": "4.7.2",
    "postcss-loader": "2.1.1",
    "prop-types": "15.6.1",
    "raf": "3.4.0",
    "react-hot-loader": "3.1.3",
    "react-test-renderer": "16.2.0",
    "redux-immutable-state-invariant": "2.1.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.2",
    "url-loader": "0.6.2",
    "webpack": "4.1.1",
    "webpack-bundle-analyzer": "2.11.0",
    "webpack-cli": "2.0.12",
    "webpack-hot-middleware": "2.21.0",
    "webpack-md5-hash": "0.0.6",
    "webpack-serve": "0.2.0"
  },
  "jest": {
    "transform": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "env",
          "react",
          "react-hmre",
          "stage-1"
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "es2015": {
                "modules": false
              },
              "targets": {
                "ie": 9,
                "uglify": true
              }
            }
          ],
          "react",
          "stage-1"
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "env",
          "react",
          "stage-1"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "globals": {},
    "rules": {
      "no-use-before-define": [2, { "functions": false }],
      "import/no-extraneous-dependencies": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  }
}
